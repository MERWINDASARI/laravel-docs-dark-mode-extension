{
  "manifest_version": 2,

  "name": "Laravel Docs Dark Mode",
  "description": "A description of the Extension",
  "version": "1.0",

  "permissions": [
    "storage"
  ],
  "content_scripts": [{
    "css": ["styles.css"],
    "matches": ["https://laravel.com/docs/*"]
  }],

  "browser_action": {
    "default_title": "Laravel Docs Dark Mode",
    "default_icon": "images/logo.png",
    "default_popup": "popup/popup.html"
  }
}